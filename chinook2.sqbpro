SELECT count(TrackId), AlbumId, TrackId FROM tracks GROUP BY AlbumId ORDER BY TrackId DESC;
SELECT name,max(Milliseconds), min(Milliseconds) FROM tracks; 
SELECT * FROM tracks WHERE Milliseconds &lt; (SELECT AVG(Milliseconds) FROM tracks);
SELECT Composer, count(Composer) FROM tracks GROUP BY Composer;
